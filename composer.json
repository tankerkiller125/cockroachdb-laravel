{
    "name": "anoixis/cockroachdb-laravel",
    "description": "CockroachDB driver for Laravel 7.x",
    "keywords": ["cockroach", "cockroachdb", "laravel"],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Nikolaj Boel Jensen",
            "email": "nbj@cego.dk"
        },
        {
            "name": "Torben Leuschner",
            "email": "torben@torbenleuschner.de"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-pdo": "*",
        "doctrine/dbal": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Anoixis\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anoixis\\Cockroach\\CockroachServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
